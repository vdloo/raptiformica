{
  "module_prototype":
  {
    "vagrant_compute_implementation":
    {
      "prototype": "compute_implementation",
      "commands":
      {
        "start_instance_command": {
          "content": "vagrant up --provider=virtualbox"
        },
        "get_hostname_command": {
          "content": "vagrant ssh-config | grep HostName | awk '{print$NF}'"
        },
        "get_port_command": {
          "content": "vagrant ssh-config | grep Port | awk '{print$NF}'"
        },
        "detect_stale_instance_command": {
          "content": "vagrant status | grep running"
        },
        "clean_up_instance_command": {
          "content": "vagrant destroy -f"
        }
      }
    }
  }
}
